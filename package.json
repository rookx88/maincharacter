{
  "name": "heirloom",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist/",
    "clean:modules": "rm -rf node_modules && rm -rf src/client/node_modules",
    "build:client": "cd src/client && npm run build",
    "build:server": "tsc -p tsconfig.json",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/app.js",
    "dev": "npm run build && cross-env NODE_NO_WARNINGS=1 nodemon --watch src --ext ts,json --exec \"tsx src/app.ts\"",
    "dev:watch": "npm run build:client && cross-env NODE_NO_WARNINGS=1 nodemon --watch src --ext ts,json --exec \"tsx src/app.ts\"",
    "seed:agents": "tsx src/scripts/seedAgents.ts",
    "migrate:agents": "tsx src/scripts/migrateAgents.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/core": "^0.1.26",
    "@langchain/openai": "^0.0.14",
    "@types/axios": "^0.14.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.28",
    "mongoose": "^8.1.3",
    "openai": "^4.28.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.17.17",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts"
    ],
    "testMatch": [
      "**/src/tests/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "_moduleAliases": {
    "@/*": "./src/*"
  }
}
